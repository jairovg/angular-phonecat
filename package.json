    "extract-text-webpack-plugin": "2.1.0",
    "http-server": "^0.9.0",
    "file-loader": "0.11.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.8",
